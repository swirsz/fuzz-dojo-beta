#!/opt/pwn.college/bash
export PROJECT_NAME=openjpeg
sudo cp /usr/local/sbin/loc_count /usr/local/sbin/fi /challenge
sudo chmod 4755 /challenge/loc_count /challenge/fi
/challenge/loc_count
cd /src/$PROJECT_NAME
rm -rf $SRC/openjpeg/data
rm -rf $SRC/openjpeg/build
cp /home/hacker/$PROJECT_NAME/*.cpp /src/$PROJECT_NAME/tests/fuzzers
/challenge/fi $PROJECT_NAME
