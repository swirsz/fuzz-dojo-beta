#!/bin/sh
. /usr/local/sbin/e
export PROJECT_NAME=openssh
[ -z "$SANITIZER" ] && export SANITIZER='none'
cd $SRC

mkdir /src/tmp
cp -f /src/openssh/regress/misc/fuzz-harness/* /src/tmp
rm -rf /src/$PROJECT_NAME
#apt-get update && apt-get install -y make autoconf automake libtool
#apt-get install -y libz-dev libssl1.1 libssl-dev libedit-dev zip
git clone --depth 1 https://github.com/openssh/openssh-portable openssh
git clone --depth 1 https://github.com/djmdjm/openssh-fuzz-cases
mv -f /src/tmp/* /src/openssh/regress/misc/fuzz-harness
rmdir /src/tmp

cp /opt/oss-fuzz/projects/$PROJECT_NAME/build.sh $SRC/
cd $SRC/$PROJECT_NAME
if [ ! -d "/home/hacker/$PROJECT_NAME" ]; then
   mkdir ~/$PROJECT_NAME
   cp $SRC/$PROJECT_NAME/yyyyyyyyyyyy/zzzzzzzzzzzzzzzzz.c $SRC/$PROJECT_NAME/yyyyyyyyyyyy/newfuzzer.c
   cp $SRC/$PROJECT_NAME/yyyyyyyyyyyy/*.c ~/$PROJECT_NAME
else
   cp ~/$PROJECT_NAME/*.c $SRC/$PROJECT_NAME/yyyyyyyyyyyy
fi
#patch -tuN /src/build.sh -i /challenge/build.patch -r -
compile
