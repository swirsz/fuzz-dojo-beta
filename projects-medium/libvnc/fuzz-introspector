#!/opt/pwn.college/bash
export PROJECT_NAME=libvnc
/challenge/.loc_count
cd /src
rm -rf /src/libvncserver
#git clone --depth 1 https://github.com/LibVNC/libvncserver.git
cp -r /opt/fuzz-targets/libvncserver /src

