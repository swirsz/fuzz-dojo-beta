cp /home/hacker/$PROJECT_NAME/*.c /src/libvncserver/test
cp /home/hacker/$PROJECT_NAME/CMakeLists.txt /src/libvncserver
cd /src/libvncserver
/challenge/.fi $PROJECT_NAME
