#!/usr/local/bin/python

from pathlib import Path
import random
import string
import shutil

with open("/flag") as f:
    random.seed(f.read())

init_path = Path(__file__)

chosen_subdir = random.randint(0, 6)
subdir_path = Path("/challenge") / str(chosen_subdir)

for file_path in subdir_path.iterdir():
    shutil.move(str(file_path), str(Path("/challenge") / file_path.name))
    
init_path.unlink()
