#!/usr/local/bin/python

import pathlib
import random
import string
import shutil

with open("/flag") as f:
    random.seed(f.read())

init_path = pathlib.Path(__file__)

chosen_subdir = random.randint(0, 6)
subdir_path = pathlib.Path.cwd() / str(chosen_subdir)

for file_path in subdir_path.iterdir():
    shutil.move(str(file_path), str(pathlib.Path.cwd() / file_path.name))

init_path.unlink()
